  },
  "homepage": "https://github.com/Leeds-eBooks/rapt#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "flow-bin": "^0.65.0",
    "flow-typed": "^2.3.0",
    "jest": "^22.2.1",
    "release-it": "^7.0.0"
  },
  "prettier": {
